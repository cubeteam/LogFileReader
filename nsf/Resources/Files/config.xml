<config>
	<logfiles>
		<file type="TXT">
			<title>Console</title>
			<filename>[data]\IBM_TECHNICAL_SUPPORT\console*.log</filename>
			<filters>
				<include>HTTP</include>
				<include>JVM</include>
				<include>XSP</include>
			</filters>
		</file>
		<file type="XML">
			<title>Error files</title>
			<filename>[data]\domino\workspace\logs\error-log-*.xml</filename>
			<transform>transform.xsl</transform>
		</file>
		<file type="XML">
			<title>Trace</title>
			<filename>[data]\domino\workspace\logs\trace-log-*.xml</filename>
			<transform>transform.xsl</transform>
		</file>
		<file type="TXT">
			<title>System info</title>
			<filename>[data]\IBM_TECHNICAL_SUPPORT\sysinfo*.log</filename>
		</file>
		<file type="TXT">
			<title>XPages</title>
			<filename>[data]\IBM_TECHNICAL_SUPPORT\xpages*.log</filename>
		</file>
		<file type="NSF">
			<title>log.nsf</title>
			<filename>log.nsf</filename>
			<viewname>($SearchEventsView)</viewname>
			<date-formula>StartTime</date-formula>
			<body-field>body</body-field>
		</file>
	</logfiles>
	<configfiles>
		<file type="TXT">
			<title>notes.ini</title>
			<filename>[config]\notes.ini</filename>
		</file>
		<file type="TXT">
			<title>java.policy</title>
			<filename>[program]\jvm\lib\security\java.policy</filename>
		</file>
		<file type="TXT">
			<title>java.pol</title>
			<filename>[program]\jvm\lib\security\java.pol</filename>
		</file>
		<file type="TXT">
			<title>rcpinstall.properties</title>
			<filename>[data]\domino\workspace\.config\rcpinstall.properties</filename>
		</file>
	</configfiles>

	<menu>
		<item name="Logs" id="logs" page="Reader.xsp">
			<action type="link" url="Reader.xsp?tab=logs" />
			<item name="Console">
				<action type="textreader">
					<filename>[data]\IBM_TECHNICAL_SUPPORT\console*.log</filename>
					<filters>
						<include>HTTP</include>
						<include>JVM</include>
						<include>XSP</include>
					</filters>
				</action>
			</item>
			<item name="System info">
				<action type="textreader">
					<filename>[data]\IBM_TECHNICAL_SUPPORT\sysinfo*.log</filename>
				</action>
			</item>
			<item name="XPages">
				<action type="textreader">
					<filename>[data]\IBM_TECHNICAL_SUPPORT\xpages*.log</filename>
				</action>
			</item>
			
		</item>
		<item name="Configuration" id="config" page="Reader.xsp">
			<action type="link" url="Reader.xsp?tab=config" />
			<item name="notes.ini">
				<action type="textreader">
					<filename>[config]\notes.ini</filename>
				</action>
			</item>
			<item name="java.policy">
				<action type="textreader">
					<filename>[program]\jvm\lib\security\java.policy</filename>
				</action>
			</item>
			<item name="java.pol">
				<action type="textreader">
					<filename>[program]\jvm\lib\security\java.pol</filename>
				</action>
			</item>
			<item name="rcpinstall.properties">
				<action type="textreader">
					<filename>[data]\domino\workspace\.config\rcpinstall.properties</filename>
				</action>
			</item>
			
			<item name="XPages">
				<action type="textreader" multiple="true">
					<filename>[data]\IBM_TECHNICAL_SUPPORT\xpages*.log</filename>
				</action>
			</item>
		</item>
		<item name="Ext" id="ext" link="#">
		</item>
		<item name="OpenLog" id="openlog" link="#">
		</item>
		<item name="About" page="About.xsp">
			<action type="link" url="About.xsp?tab=about" />
		</item>
		<item name="Majkilde">
			<action type="link" url="http://www.majkilde.dk" />
		</item>
	</menu>
</config>